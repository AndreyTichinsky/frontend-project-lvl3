{
  "env": {
      "browser": true,
      "es2021": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
      "ecmaVersion": 12,
      "sourceType": "module"
  },
  "rules": {
    "no-var": "error",
    "import/prefer-default-export": "off",
    "no-console": "off",
    "no-underscore-dangle": [
      2,
      {
        "allow": [
          "__filename",
          "__dirname"
        ]
      }
    ],
    "import/extensions": [
      "error",
      "ignorePackages",
      {
        "js": "always",
        "jsx": "always",
        "ts": "always",
        "tsx": "always"
      }
    ]
  }
}
