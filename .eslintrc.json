{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true,
    "jest": true
  },
  "extends": ["eslint:recommended", "airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "no-var": "error",
    "import/prefer-default-export": "off",
    "no-underscore-dangle": [
      2,
      {
        "allow": ["__filename", "__dirname"]
      }
    ]
  }
}
